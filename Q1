#include <stdio.h>

int main(){
  int a, b, c, d, dif;

  printf("digite o valor de 'a':");
  scanf("%d", &a);

  printf("digite o valor de 'b':");
  scanf("%d", &b);

  printf("digite o valor de 'c':");
  scanf("%d", &c);

  printf("digite o valor de 'd':");
  scanf("%d", &d);

  dif = (a*b)-(c*d);

  printf("Diferenca = (%d * %d - %d * %d)\n", a,b ,c ,d);

  printf("Diferenca = (%d)", dif);

  return 0;
  
  
}
