#include <stdio.h>

int main(){
  float x,y;
  
  printf("Digite x e y em sequencia:");
  scanf("%f %f", &x, &y);

  if(x==0 && y==0){
    printf("Origem\n");
    
  }else if (x==0 && y!=0){
      printf("Eixo y\n");
   
  }else if (x==0 && y!=0){
         printf("Eixo x\n");

  }else if(x>0 && y>0){
          printf("quadrante Q1\n");
  }else if(x<0 && y>0){
          printf("quadrante Q2\n");
  }else if(x<0 && y<0){
           printf("quadrante Q3\n");
  }else if (x>0 && y<0){
           printf("quadrante Q4\n");
  }
    return 0;
}
